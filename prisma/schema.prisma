generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Template {
  id        Int      @id @default(autoincrement())
  name      String
  category  String
  desc      String
  tag       String?
  imageUrl  String?
  demoUrl   String?
  useUrl    String?
  createdAt DateTime @default(now())
}
model Transaction {
  id        Int      @id @default(autoincrement())
  nama      String
  email     String
  paket     String
  harga     String
  metode    String
  createdAt DateTime @default(now())
}